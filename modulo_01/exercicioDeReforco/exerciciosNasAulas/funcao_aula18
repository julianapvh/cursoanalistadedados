def soma(num1,num2):
    return print('A soma dos números é: ', num1 + num2)

def subtracao(num1,num2):
    return print('A subtração dos números é: ', num1 - num2)

def multiplicacao(num1,num2):
    return print('A multiplicação dos números é: ', num1 * num2)

def divisao(num1,num2):
    return print('A divisão dos números é: ', num1 / num2)

def divisao_semsobra(num1,num2):
    return print('A divisão sem sobra dos números é: ', num1 // num2)

def divisao_comsobra(num1,num2):
    return print('A divisão com sobra dos números é: ', num1 % num2)